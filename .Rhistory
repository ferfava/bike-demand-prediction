library(tidyverse)
library(lubridate)
library(keras)
library(tensorflow)
library(recipes)
library(rsample)
library(yardstick)
library(vip)
#  3. Cargar los datos
data <- read_csv("hour.csv")
#  4. Exploraci贸n r谩pida
glimpse(data)
summary(data$cnt)
# Ъ 5. Preprocesamiento y feature engineering
data_clean <- data %>%
mutate(
hr_sin = sin(2 * pi * hr / 24),
hr_cos = cos(2 * pi * hr / 24),
weekday = as.factor(weekday),
season = as.factor(season),
weathersit = as.factor(weathersit),
workingday = as.factor(workingday)
) %>%
select(cnt, temp, atemp, hum, windspeed, hr_sin, hr_cos,
season, weathersit, workingday)
# И 6. Dividir en entrenamiento y prueba
set.seed(123)
split <- initial_split(data_clean, prop = 0.8)
train_data <- training(split)
test_data  <- testing(split)
#  7. Preparar receta (normalizaci贸n + dummies)
rec <- recipe(cnt ~ ., data = train_data) %>%
step_dummy(all_nominal()) %>%
step_normalize(all_predictors()) %>%
prep()
x_train <- bake(rec, new_data = train_data) %>% select(-cnt) %>% as.matrix()
y_train <- train_data$cnt
x_test <- bake(rec, new_data = test_data) %>% select(-cnt) %>% as.matrix()
y_test <- test_data$cnt
#  8. Crear la red neuronal
model <- keras_model_sequential() %>%
layer_dense(units = 128, activation = "relu", input_shape = ncol(x_train)) %>%
layer_dropout(rate = 0.3) %>%
layer_dense(units = 64, activation = "relu") %>%
layer_dense(units = 1)
model %>% compile(
loss = "mse",
optimizer = optimizer_adam(learning_rate = 0.001),
metrics = c("mae")
)
#  9. Entrenar el modelo
history <- model %>% fit(
x = x_train,
y = y_train,
epochs = 100,
batch_size = 64,
validation_split = 0.2,
callbacks = callback_early_stopping(patience = 10, restore_best_weights = TRUE)
)
#  10. Evaluar el modelo
model %>% evaluate(x_test, y_test)
preds <- model %>% predict(x_test) %>% as.vector()
#  11. M茅tricas
tibble(obs = y_test, pred = preds) %>%
metrics(truth = obs, estimate = pred)
#  12. Visualizar predicciones vs reales
ggplot(data.frame(y_test, preds), aes(x = y_test, y = preds)) +
geom_point(alpha = 0.4, color = "#00BFC4") +
geom_abline(slope = 1, intercept = 0, color = "red", linetype = "dashed") +
labs(title = "Predicci贸n vs Real", x = "Valor real", y = "Valor predicho") +
theme_minimal()
#  13. Feature Importance (VIP)
vip(model, num_features = 10, method = "permute", train = x_test, target = y_test)
#  13. Feature Importance (VIP)
# Convertir x_test a data.frame con nombres de columnas
x_test_df <- bake(rec, new_data = test_data) %>% select(-cnt)
# Usar vip con m茅trica expl铆cita (ej: "mae")
vip(
object = model,
num_features = 10,
method = "permute",
train = x_test_df,
target = y_test,
metric = "mae"
)
#  13. Feature Importance (VIP)
# Convertir x_test a data.frame con nombres de columnas
x_test_df <- bake(rec, new_data = test_data) %>% select(-cnt)
# Usar vip con m茅trica expl铆cita y especificar la funci贸n de predicci贸n
vip(
object = model,
num_features = 10,
method = "permute",
train = x_test_df,
target = y_test,
metric = "mae",
pred_wrapper = function(model, X) predict(model, X)  # Funci贸n para hacer predicciones
)
#  13. Feature Importance (VIP)
# Convertir x_test a data.frame con nombres de columnas
x_test_df <- bake(rec, new_data = test_data) %>% select(-cnt)
# Usar vip con m茅trica expl铆cita y funci贸n de predicci贸n con 'newdata'
vip(
object = model,
num_features = 10,
method = "permute",
train = x_test_df,
target = y_test,
metric = "mae",
pred_wrapper = function(object, newdata) {
predict(object, as.matrix(newdata)) %>% as.vector()
}
)
